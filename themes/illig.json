{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": false
          },
          "style": "powerline",
          "template": "[\u2327 {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf42e{{ end }}]",
          "foreground": "#ff0000",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "status"
        },
        {
          "properties": {
            "root_icon": "â˜ "
          },
          "style": "powerline",
          "template": " \uf0e7 ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#ff0000",
          "type": "root"
        },
        {
          "style": "powerline",
          "template": " {{ if .SSHSession }}\udb80\udf18 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#1f1f1f",
          "type": "session"
        },
        {
          "properties": {
            "style": "short"
          },
          "style": "powerline",
          "template": " \udb80\ude4b {{ .Path }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#0000c0",
          "type": "path"
        },
        {
          "style": "powerline",
          "template": "{{ if .Env.LOCATION_STACK_DEPTH }} \ue705 {{ .Env.LOCATION_STACK_DEPTH }} {{ end }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#0077c2",
          "type": "text"
        },
        {
          "style": "powerline",
          "template": "{{ if .Env.VSPROMPTVERSION }} \u2302 vs{{ .Env.VSPROMPTVERSION }} {{ end }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#720dcf",
          "type": "text"
        },
        {
          "properties": {
            "display_mode": "always",
            "home_enabled": true
          },
          "style": "powerline",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#00ffff",
          "type": "dotnet"
        },
        {
          "style": "powerline",
          "template": " \u2388 {{ replaceP \"([a-f0-9]{2})[a-f0-9]{6}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{10}([a-f0-9]{2})\" .Context \"$1..$2\" }}{{ if .Namespace }} :: {{ .Namespace }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#ebcc34",
          "type": "kubectl"
        },
        {
          "style": "powerline",
          "template": " \udb82\udc05 {{ .Name }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#3493eb",
          "type": "az"
        },
        {
          "style": "powerline",
          "template": "\ue7b2 {{.Account}}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#571486",
          "type": "gcp"
        },
        {
          "properties": {
            "fetch_status": true
          },
          "style": "powerline",
          "template": " {{.Env.PRE_COMMIT_INSTALLED}}{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \udb80\udd93 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#00cc00",
          "type": "git"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "foreground": "#ffffff",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": " {{ .Name }}>",
          "type": "shell"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
